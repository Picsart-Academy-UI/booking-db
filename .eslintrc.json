{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "consistent-return": "off",
        "camelcase": "off",
        "no-underscore-dangle": "off",
        "comma-dangle": "off",
        "new-cap": "off",
        "prefer-destructuring": ["error", {
        "object": true,
        "array": false
        }],
        "no-unused-vars": "off"
    }
}
